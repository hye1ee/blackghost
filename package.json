{
  "name": "blackghost",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.18.6",
    "@electron/remote": "^2.0.8",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "dmg-builder": "^23.5.1",
    "electron-is-dev": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-moving-text": "^0.0.7",
    "react-scripts": "5.0.1",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none yarn start\" \"yarn electron:start\"",
    "electron:start": "wait-on tcp:3000 && electron .",
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=build/electron.js",
    "electron:build:new": "yarn build && electron-builder -c.extraMetadata.main=build/electron.js --publish=always",
    "electron:build:macos": "yarn build && electron-builder -c.extraMetadata.main=build/electron.js --arm64"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "extends": null,
    "appId": "com.example.electron-cra",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "./build/",
      "output": "./dist/"
    }
  },
  "devDependencies": {
    "electron": "20.2.0",
    "electron-builder": "^23.3.3"
  }
}